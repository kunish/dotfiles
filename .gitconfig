[init]
  defaultBranch = main
[core]
  ignorecase = false
[user]
  name = shikun
  email = kunish.butt@gmail.com
	signingkey = 2C564E822E7477FC4F6FF2F067D3ACD788F3A7CD
[interactive]
  diffFilter = delta --color-only
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[pager]
  diff = delta
  log = delta
  reflog = delta
  show = delta
[delta]
  syntax-theme = gruvbox-dark
  navigate = true
[commit]
	gpgsign = true
[pull]
  rebase = true
